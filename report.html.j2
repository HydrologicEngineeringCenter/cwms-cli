<!doctype html>
<html>
  <head><meta charset="utf-8"/><title>{{ title }}</title></head>
  <body>
    <h1>{{ title }}</h1>
    <p>Office: {{ office }}</p>
    <p>Generated at: {{ generated_at }}</p>

    <h2>Latest values</h2>
    <ul>
    {% for item in series %}
      <li><strong>{{ item.name }}</strong>: {{ item.value }} {{ item.unit }} at {{ item.time }}</li>
    {% endfor %}
    </ul>
  </body>
</html>
